<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android" android:layout_width="match_parent"
    android:layout_height="match_parent">

    <TextView
        android:id="@+id/title"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:layout_margin="15dp"
        android:textSize="20sp"
        android:text="Special Thanks! "/>


    <TextView
        android:id="@+id/launcher"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:layout_below="@id/title"
        android:layout_margin="10dp"
        android:text="Launcher icon made by https://www.flaticon.com/authors/freepik from www.flaticon.com "/>

    <TextView
        android:id="@+id/settingsicon"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:layout_below="@id/launcher"
        android:layout_margin="10dp"
        android:text="Calculator icon made by https://www.flaticon.com/authors/smashicons from www.flaticon.com "/>

    <TextView
        android:id="@+id/peopleicon"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:layout_below="@id/settingsicon"
        android:layout_margin="10dp"
        android:text="People icon made by https://www.flaticon.com/authors/egor-rumyantsev from www.flaticon.com "/>

    <TextView
        android:id="@+id/calc"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:layout_below="@id/peopleicon"
        android:layout_margin="10dp"
        android:text="Settings icon made by https://www.flaticon.com/authors/daniel-bruce from www.flaticon.com "/>




</RelativeLayout>